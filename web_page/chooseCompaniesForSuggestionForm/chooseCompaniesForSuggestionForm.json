        "\t$data.premiumHiringApplication.interview = $data.premiumHiringApplication_interview;",
        "\treturn {",
        "\t\t//map premiumHiringApplication variable to expected task contract input",
        "\t\tpremiumHiringApplicationInput: {",
        "\t\t\trecommendedCompanies: $data.premiumHiringApplication.recommendedCompanies.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null",
