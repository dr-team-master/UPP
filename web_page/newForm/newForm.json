      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.premiumHiringApplication && $data.premiumHiringApplication_recommendedCompanies && $data.premiumHiringApplication_selectedCompany && $data.premiumHiringApplication_interview ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.premiumHiringApplication.recommendedCompanies = $data.premiumHiringApplication_recommendedCompanies;",
        "\t$data.premiumHiringApplication.selectedCompany = $data.premiumHiringApplication_selectedCompany;",
        "\t$data.premiumHiringApplication.interview = $data.premiumHiringApplication_interview;",
        "\treturn {",
        "\t\t//map premiumHiringApplication variable to expected task contract input",
        "\t\tpremiumHiringApplicationInput: {",
        "\t\t\trecommendedCompanies: $data.premiumHiringApplication.recommendedCompanies.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null",
        "\t\t\t}))",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
